{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "defaultValue": "northeurope",
            "type": "String",
            "metadata": {
                "description": "Location for the VMs, only certain regions support zones during preview."
            }
        },
        "networkInterfaceName1": {
            "defaultValue": "test-sql-ni1198",
            "type": "String"
        },
        "enableAcceleratedNetworking": {
            "defaultValue": false,
            "type": "Bool"
        },
        "networkSecurityGroupName": {
            "defaultValue": "test-SQLvm-nsg",
            "type": "String"
        },
        "networkSecurityGroupRules": {
            "defaultValue": [],
            "type": "Array"
        },
        "subnetName": {
            "type": "String"
        },
        "virtualNetworkId": {
            "type": "String"
        },
        "virtualMachineName1": {
            "defaultValue": "test-SQLvm",
            "type": "String"
        },
        "virtualMachineComputerName1": {
            "defaultValue": "test-SQLvm",
            "type": "String"
        },
        "virtualMachineZone": {
            "type": "Array",
            "metadata": {
                "description": "The virtual machine's availability zone."
            }
        },
        "osDiskType": {
            "defaultValue": "Premium_LRS",
            "type": "String"
        },
        "osDiskDeleteOption": {
            "defaultValue": "Delete",
            "type": "String"
        },
        "dataDisks1": {
            "defaultValue": [
                {
                    "lun": 0,
                    "createOption": "attach",
                    "deleteOption": "Detach",
                    "caching": "ReadOnly",
                    "writeAcceleratorEnabled": false,
                    "id": null,
                    "name": "test-SQLvm_DataDisk_0",
                    "storageAccountType": null,
                    "diskSizeGB": null,
                    "diskEncryptionSet": null
                },
                {
                    "lun": 1,
                    "createOption": "attach",
                    "deleteOption": "Detach",
                    "caching": "None",
                    "writeAcceleratorEnabled": false,
                    "id": null,
                    "name": "test-SQLvm_DataDisk_1",
                    "storageAccountType": null,
                    "diskSizeGB": null,
                    "diskEncryptionSet": null
                }
            ],
            "type": "Array"
        },
        "dataDiskResources1": {
            "defaultValue": [
                {
                    "name": "test-SQLvm_DataDisk_0",
                    "sku": "Premium_LRS",
                    "properties": {
                        "diskSizeGB": 1024,
                        "creationData": {
                            "createOption": "empty"
                        }
                    }
                },
                {
                    "name": "test-SQLvm_DataDisk_1",
                    "sku": "Premium_LRS",
                    "properties": {
                        "diskSizeGB": 1024,
                        "creationData": {
                            "createOption": "empty"
                        }
                    }
                }
            ],
            "type": "Array"
        },
        "virtualMachineSize": {
            "defaultValue": "Standard_B2s",
            "type": "String",
            "metadata": {
                "description": "VM Type in Azure: Standard_B2s or Standard_A2_v2 or Standard_F2s_v2, etc."
            }
        },
        "nicDeleteOption": {
            "defaultValue": "Detach",
            "type": "String"
        },
        "adminUsername": {
            "type": "String",
            "metadata": {
                "description": "Username for the local admin in the Virtual Machine."
            }
        },
        "adminPassword": {
            "type": "SecureString"
        },
        "enableHotpatching": {
            "defaultValue": true,
            "type": "Bool",
            "metadata": {
                "description": "true"
            }
        },
        "patchMode": {
            "defaultValue": "AutomaticByPlatform",
            "type": "String",
            "metadata": {
                "description": "Choose AutomaticByPlatform to enable Hotpatching otherwise, Manual."
            }
        },
        "rebootSetting": {
            "value": "Never"
        },
        "diagnosticsStorageAccountName": {
            "type": "String"
        },
        "diagnosticsStorageAccountId": {
            "type": "String"
        },
        "sqlVirtualMachineLocation": {
            "defaultValue": "northeurope",
            "type": "String"
        },
        "sqlVirtualMachineName": {
            "defaultValue": "test-SQLvm",
            "type": "String"
        },
        "sqlManagement": {
            "defaultValue": "Full",
            "type": "String",
            "metadata": {
                "description": "SQL Server Management type. Values: Full, LightWeight, NoAgent"
            }
        },
        "sqlVMleastPrivilegeMode": {
            "defaultValue": "Enabled",
            "type": "String",
            "metadata": {
                "description": "SQL IaaS Agent least privilege mode. Can be set to Enabled, otherwise to NotSet"
            }
        },
        "sqlConnectivityType": {
            "defaultValue": "Private",
            "type": "String"
        },
        "sqlPortNumber": {
            "defaultValue": 1433,
            "type": "Int"
        },
        "sqlStorageWorkloadType": {
            "defaultValue": "OLTP",
            "type": "String"
        },
        "sqlStorageDisksConfigurationType": {
            "defaultValue": "NEW",
            "type": "String"
        },
        "sqlAutopatchingDayOfWeek": {
            "defaultValue": "Sunday",
            "type": "String"
        },
        "sqlAutopatchingStartHour": {
            "defaultValue": "2",
            "type": "String"
        },
        "sqlAutopatchingWindowDuration": {
            "defaultValue": "60",
            "type": "String"
        },
        "sqlAutobackupRetentionPeriod": {
            "defaultValue": "30",
            "type": "String"
        },
        "sqlAutobackupStorageAccountName": {
            "type": "String"
        },
        "sqlAutobackupStorageContainerName": {
            "defaultValue": "sqlvmtest",
            "type": "String"
        },
        "backupSystemDbs": {
            "defaultValue": "true",
            "type": "String"
        },
        "backupScheduleType": {
            "defaultValue": "Automated",
            "type": "String"
        },
        "dataPath": {
            "defaultValue": "F:\\data",
            "type": "String"
        },
        "dataDisksLUNs": {
            "defaultValue": [
                0
            ],
            "type": "Array"
        },
        "logPath": {
            "defaultValue": "G:\\log",
            "type": "String"
        },
        "logDisksLUNs": {
            "defaultValue": [
                1
            ],
            "type": "Array"
        },
        "tempDbPath": {
            "defaultValue": "D:\\tempDb",
            "type": "String"
        },
        "dataFileCount": {
            "defaultValue": 4,
            "type": "Int"
        },
        "dataFileSize": {
            "defaultValue": 8,
            "type": "Int"
        },
        "dataGrowth": {
            "defaultValue": 64,
            "type": "Int"
        },
        "logFileSize": {
            "defaultValue": 8,
            "type": "Int"
        },
        "logGrowth": {
            "defaultValue": 64,
            "type": "Int"
        },
        "SQLSystemDbOnDataDisk": {
            "defaultValue": false,
            "type": "Bool"
        },
        "rServicesEnabled": {
            "defaultValue": "false",
            "type": "String"
        },
        "maxdop": {
            "defaultValue": 0,
            "type": "Int"
        },
        "isOptimizeForAdHocWorkloadsEnabled": {
            "defaultValue": false,
            "type": "Bool"
        },
        "collation": {
            "defaultValue": "SQL_Latin1_General_CP1_CI_AS",
            "type": "String"
        },
        "minServerMemoryMB": {
            "defaultValue": 0,
            "type": "Int"
        },
        "maxServerMemoryMB": {
            "defaultValue": 2147483647,
            "type": "Int"
        },
        "isLPIMEnabled": {
            "defaultValue": false,
            "type": "Bool"
        },
        "isIFIEnabled": {
            "defaultValue": false,
            "type": "Bool"
        },
        "licenseType": {
            "defaultValue": "Windows_Server",
            "type": "String",
            "metadata": {
                "description": "licenseType must be Windows_Server to activate Azure Hybrid Benefit for Windows Server, otherwise None"
            }
        },
        "SqlServerLicenseType": {
            "defaultValue": "AHUB",
            "type": "String",
            "metadata": {
                "description": "licenseType must be AHUB to activate Azure Hybrid Benefit for Windows Server, otherwise PAYG"
            }
        },
        "KeyVaultCredentialName": {
            "defaultValue": "",
            "type": "String"
        },
        "KeyVaultCredentialEnable": {
            "defaultValue": false,
            "type": "Bool",
            "metadata": {
                "description": "Enable or disable key vault credential setting."
            }
        },
        "imageReference": {
            "defaultValue": 
                    {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2022-datacenter-azure-edition-core",
                        "version": "latest"
                    },
            "type": "Array",
            "metadata": {
                "description": "Image Reference for the deployment."
            }
        },
        "installscripturi": {
            "type": "String"
        },
        "command2exec": {
            "type": "String"
        },
        "ipConfigName": {
            "defaultValue": "ipconfig1",
            "type": "String",
            "metadata": {
                "description": "The name for the ip configuration of the Network Interface"
            }
        },
        "SQLAuthUpdateUserName": {
            "defaultValue": "",
            "type": "String",
            "metadata": {
                "description": ""
            }
        },
        "SQLAuthUpdatePassword": {
            "defaultValue": "",
            "type": "String",
            "metadata": {
                "description": ""
            }
        },
        "guestConfigExtTemplateLink": {
            "defaultValue": "https://catalogartifact.azureedge.net/publicartifacts/MicrosoftGuestConfiguration.ConfigurationforWindows-1.0.0/MainTemplate.json",
            "type": "String",
            "metadata": {
                "description": "URI for the guest configuration extension"
            }
        },
        "guestConfigExtVmName": {
            "defaultValue": "test-SQLvm",
            "type": "String",
            "metadata": {
                "description": "The name of the VM for guest configuration extension"
            }
        },
        "guestConfigExtLocation": {
            "defaultValue": "northeurope",
            "type": "String",
            "metadata": {
                "description": "Location for guest configuration extension"
            }
        },
        "customScriptExtensionTemplateLink": {
            "defaultValue": "https://catalogartifact.azureedge.net/publicartifacts/Microsoft.CustomScriptExtension-arm-2.0.57/MainTemplate.json",
            "type": "String",
            "metadata": {
                "description": "Arm Template Link for Custom Script Extension for Windows"
            }
        },
        "customScriptExtensionTemplateUris": {
            "defaultValue": "https://sqlvmautobackupstorage.blob.core.windows.net/scripts/configure-server.ps1",
            "type": "String",
            "metadata": {
                "description": "File URI for Custom Script Extension for Windows"
            }
        },
        "customScriptExtensionVmName": {
            "defaultValue": "test-SQLvm",
            "type": "String",
            "metadata": {
                "description": "The name of the VM for Custom Script Extension for Windows"
            }
        },
        "customScriptExtensionLocation": {
            "defaultValue": "northeurope",
            "type": "String",
            "metadata": {
                "description": "Location for Custom Script Extension for Windows"
            }
        },
        "diagnosticsStorageAccountEndPoint": {
            "defaultValue": "https://core.windows.net/",
            "type": "String",
            "metadata": {
                "description": "Service End Point for Diagnostics Storage Account"
            }
        }
    },
    "variables": {
        "nsgId": "[resourceId(resourceGroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroupName'))]",
        "vnetId": "[parameters('virtualNetworkId')]",
        "vnetName": "[last(split(variables('vnetId'), '/'))]",
        "subnetRef": "[concat(variables('vnetId'), '/subnets/', parameters('subnetName'))]",
        "diagnosticsExtensionName": "Microsoft.Insights.VMDiagnosticsSettings"
    },
    "resources": [
        {
            "type": "Microsoft.Network/networkInterfaces",
            "apiVersion": "2021-08-01",
            "name": "[parameters('networkInterfaceName1')]",
            "location": "[parameters('location')]",
            "dependsOn": [
                "[concat('Microsoft.Network/networkSecurityGroups/', parameters('networkSecurityGroupName'))]"
            ],
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "[parameters('ipConfigName')]",
                        "properties": {
                            "subnet": {
                                "id": "[variables('subnetRef')]"
                            },
                            "privateIPAllocationMethod": "Dynamic"
                        }
                    }
                ],
                "enableAcceleratedNetworking": "[parameters('enableAcceleratedNetworking')]",
                "networkSecurityGroup": {
                    "id": "[variables('nsgId')]"
                }
            }
        },
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2019-02-01",
            "name": "[parameters('networkSecurityGroupName')]",
            "location": "[parameters('location')]",
            "properties": {
                "securityRules": "[parameters('networkSecurityGroupRules')]"
            }
        },
        {
            "type": "Microsoft.Compute/disks",
            "apiVersion": "2022-03-02",
            "name": "[parameters('dataDiskResources1')[copyIndex()].name]",
            "location": "[parameters('location')]",
            "sku": {
                "name": "[parameters('dataDiskResources1')[copyIndex()].sku]"
            },
            "zones": "[if(contains(parameters('dataDiskResources1')[copyIndex()].sku, '_ZRS'), json('null'), array(1))]",
            "properties": "[parameters('dataDiskResources1')[copyIndex()].properties]",
            "copy": {
                "name": "managedDiskResources",
                "count": "[length(parameters('dataDiskResources1'))]"
            }
        },
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2022-03-01",
            "name": "[parameters('virtualMachineName1')]",
            "location": "[parameters('location')]",
            "dependsOn": [
                "managedDiskResources",
                "[concat('Microsoft.Network/networkInterfaces/', parameters('networkInterfaceName1'))]"
            ],
            "zones": "[parameters('virtualMachineZone')]",
            "identity": {
                "type": "systemAssigned"
            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "[parameters('virtualMachineSize')]"
                },
                "storageProfile": {
                    "osDisk": {
                        "createOption": "fromImage",
                        "managedDisk": {
                            "storageAccountType": "[parameters('osDiskType')]"
                        },
                        "deleteOption": "[parameters('osDiskDeleteOption')]"
                    },
                    "imageReference": "[parameters('imageReference')]",
                    "copy": [
                        {
                            "name": "dataDisks",
                            "count": "[length(parameters('dataDisks1'))]",
                            "input": {
                                "lun": "[parameters('dataDisks1')[copyIndex('dataDisks')].lun]",
                                "createOption": "[parameters('dataDisks1')[copyIndex('dataDisks')].createOption]",
                                "caching": "[parameters('dataDisks1')[copyIndex('dataDisks')].caching]",
                                "diskSizeGB": "[parameters('dataDisks1')[copyIndex('dataDisks')].diskSizeGB]",
                                "managedDisk": {
                                    "id": "[coalesce(parameters('dataDisks1')[copyIndex('dataDisks')].id, if(equals(parameters('dataDisks1')[copyIndex('dataDisks')].name, json('null')), json('null'), resourceId('Microsoft.Compute/disks', parameters('dataDisks1')[copyIndex('dataDisks')].name)))]",
                                    "storageAccountType": "[parameters('dataDisks1')[copyIndex('dataDisks')].storageAccountType]"
                                },
                                "deleteOption": "[parameters('dataDisks1')[copyIndex('dataDisks')].deleteOption]",
                                "writeAcceleratorEnabled": "[parameters('dataDisks1')[copyIndex('dataDisks')].writeAcceleratorEnabled]"
                            }
                        }
                    ]
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaceName1'))]",
                            "properties": {
                                "deleteOption": "[parameters('nicDeleteOption')]"
                            }
                        }
                    ]
                },
                "securityProfile": {
                    "encryptionAtHost": true
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachineComputerName1')]",
                    "adminUsername": "[parameters('adminUsername')]",
                    "adminPassword": "[parameters('adminPassword')]",
                    "windowsConfiguration": {
                        "enableAutomaticUpdates": true,
                        "provisionVmAgent": true,
                        "patchSettings": {
                            "enableHotpatching": "[parameters('enableHotpatching')]",
                            "patchMode": "[parameters('patchMode')]",
                            "automaticByPlatformSettings": {
                                "rebootSetting": "[parameters('rebootSetting')]"
                            }
                        }
                    }
                },
                "licenseType": "[parameters('licenseType')]",
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "[reference(concat('Microsoft.Storage/storageAccounts/', parameters('diagnosticsStorageAccountName')),'2015-06-15').primaryEndpoints.blob]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.SqlVirtualMachine/SqlVirtualMachines",
            "apiVersion": "2022-07-01-preview",
            "name": "[parameters('sqlVirtualMachineName')]",
            "location": "[parameters('sqlVirtualMachineLocation')]",
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', parameters('sqlVirtualMachineName'))]"
            ],
            "properties": {
                "virtualMachineResourceId": "[resourceId('Microsoft.Compute/virtualMachines', parameters('sqlVirtualMachineName'))]",
                "sqlManagement": "[parameters('sqlManagement')]",
                "SqlServerLicenseType": "[parameters('SqlServerLicenseType')]",
                "LeastPrivilegeMode": "[parameters('sqlVMleastPrivilegeMode')]",
                "AutoPatchingSettings": {
                    "Enable": true,
                    "DayOfWeek": "[parameters('sqlAutopatchingDayOfWeek')]",
                    "MaintenanceWindowStartingHour": "[parameters('sqlAutopatchingStartHour')]",
                    "MaintenanceWindowDuration": "[parameters('sqlAutopatchingWindowDuration')]"
                },
                "AutoBackupSettings": {
                    "Enable": true,
                    "RetentionPeriod": "[parameters('sqlAutobackupRetentionPeriod')]",
                    "EnableEncryption": false,
                    "BackupSystemDbs": "[parameters('backupSystemDbs')]",
                    "BackupScheduleType": "[parameters('backupScheduleType')]",
                    "StorageAccountUrl": "[reference(concat('Microsoft.Storage/storageAccounts/', parameters('sqlAutobackupStorageAccountName')),'2015-06-15').primaryEndpoints.blob]",
                    "StorageAccessKey": "[first(listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('sqlAutobackupStorageAccountName')), '2021-01-01').keys).value]",
                    "StorageContainerName": "[parameters('sqlAutobackupStorageContainerName')]"
                },
                "KeyVaultCredentialSettings": {
                    "Enable": "[parameters('KeyVaultCredentialEnable')]",
                    "CredentialName": "[parameters('KeyVaultCredentialName')]"
                },
                "StorageConfigurationSettings": {
                    "DiskConfigurationType": "[parameters('sqlStorageDisksConfigurationType')]",
                    "StorageWorkloadType": "[parameters('sqlStorageWorkloadType')]",
                    "SQLDataSettings": {
                        "LUNs": "[parameters('dataDisksLUNs')]",
                        "DefaultFilePath": "[parameters('dataPath')]"
                    },
                    "SQLLogSettings": {
                        "LUNs": "[parameters('logDisksLUNs')]",
                        "DefaultFilePath": "[parameters('logPath')]"
                    },
                    "SQLTempDbSettings": {
                        "DefaultFilePath": "[parameters('tempDbPath')]",
                        "DataFileCount": "[parameters('dataFileCount')]",
                        "DataFileSize": "[parameters('dataFileSize')]",
                        "DataGrowth": "[parameters('dataGrowth')]",
                        "LogFileSize": "[parameters('logFileSize')]",
                        "LogGrowth": "[parameters('logGrowth')]"
                    },
                    "SQLSystemDbOnDataDisk": "[parameters('SQLSystemDbOnDataDisk')]"
                },
                "ServerConfigurationsManagementSettings": {
                    "SQLConnectivityUpdateSettings": {
                        "ConnectivityType": "[parameters('sqlConnectivityType')]",
                        "Port": "[parameters('sqlPortNumber')]",
                        "SQLAuthUpdateUserName": "[parameters('SQLAuthUpdateUserName')]",
                        "SQLAuthUpdatePassword": "[parameters('SQLAuthUpdatePassword')]"
                    },
                    "AdditionalFeaturesServerConfigurations": {
                        "IsRServicesEnabled": "[parameters('rServicesEnabled')]"
                    },
                    "SQLInstanceSettings": {
                        "maxDop": "[parameters('maxdop')]",
                        "isOptimizeForAdHocWorkloadsEnabled": "[parameters('isOptimizeForAdHocWorkloadsEnabled')]",
                        "collation": "[parameters('collation')]",
                        "minServerMemoryMB": "[parameters('minServerMemoryMB')]",
                        "maxServerMemoryMB": "[parameters('maxServerMemoryMB')]",
                        "isLPIMEnabled": "[parameters('isLPIMEnabled')]",
                        "isIFIEnabled": "[parameters('isIFIEnabled')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "name": "MicrosoftGuestConfiguration.ConfigurationforWindows",
            "dependsOn": [
                "[concat('Microsoft.Compute/virtualMachines/', parameters('virtualMachineName1'))]"
            ],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[parameters('guestConfigExtTemplateLink')]"
                },
                "parameters": {
                    "vmName": {
                        "value": "[parameters('guestConfigExtVmName')]"
                    },
                    "location": {
                        "value": "[parameters('guestConfigExtLocation')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "name": "Microsoft.CustomScriptExtension",
            "dependsOn": [
                "MicrosoftGuestConfiguration.ConfigurationforWindows"
            ],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[parameters('customScriptExtensionTemplateLink')]"
                },
                "parameters": {
                    "fileUris": {
                        "value": "[parameters('installscripturi')]"
                    },
                    "vmName": {
                        "value": "[parameters('customScriptExtensionVmName')]"
                    },
                    "location": {
                        "value": "[parameters('customScriptExtensionLocation')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "apiVersion": "2018-10-01",
            "name": "[concat(parameters('virtualMachineName1'),'/', variables('diagnosticsExtensionName'))]",
            "location": "[parameters('location')]",
            "dependsOn": [
                "Microsoft.CustomScriptExtension"
            ],
            "properties": {
                "publisher": "Microsoft.Azure.Diagnostics",
                "type": "IaaSDiagnostics",
                "typeHandlerVersion": "1.5",
                "autoUpgradeMinorVersion": true,
                "settings": {
                    "StorageAccount": "[parameters('diagnosticsStorageAccountName')]",
                    "WadCfg": {
                        "DiagnosticMonitorConfiguration": {
                            "overallQuotaInMB": 5120,
                            "Metrics": {
                                "resourceId": "[concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', resourceGroup().name, '/providers/', 'Microsoft.Compute/virtualMachines/', parameters('virtualMachineName1'))]",
                                "MetricAggregation": [
                                    {
                                        "scheduledTransferPeriod": "PT1H"
                                    },
                                    {
                                        "scheduledTransferPeriod": "PT1M"
                                    }
                                ]
                            },
                            "DiagnosticInfrastructureLogs": {
                                "scheduledTransferLogLevelFilter": "Error"
                            },
                            "PerformanceCounters": {
                                "scheduledTransferPeriod": "PT1M",
                                "PerformanceCounterConfiguration": [
                                    {
                                        "counterSpecifier": "\\Processor Information(_Total)\\% Processor Time",
                                        "unit": "Percent",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Processor Information(_Total)\\% Privileged Time",
                                        "unit": "Percent",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Processor Information(_Total)\\% User Time",
                                        "unit": "Percent",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Processor Information(_Total)\\Processor Frequency",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\System\\Processes",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Process(_Total)\\Thread Count",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Process(_Total)\\Handle Count",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\System\\System Up Time",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\System\\Context Switches/sec",
                                        "unit": "CountPerSecond",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\System\\Processor Queue Length",
                                        "unit": "Count",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Memory\\% Committed Bytes In Use",
                                        "unit": "Percent",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Memory\\Available Bytes",
                                        "unit": "Bytes",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Memory\\Committed Bytes",
                                        "unit": "Bytes",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Memory\\Cache Bytes",
                                        "unit": "Bytes",
                                        "sampleRate": "PT60S"
                                    },
                                    {
                                        "counterSpecifier": "\\Memory\\Pool Paged Bytes",
                                        "unit": "Bytes",
